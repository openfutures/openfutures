{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 7 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/7"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-xml": "*",
        "composer/installers": "^1.9",
        "composer/semver": "^1.4",
        "cweagans/composer-patches": "^1.7",
        "drupal-composer/preserve-paths": "^0.1.6",
        "drupal/admin_menu": "^3.0@RC",
        "drupal/admin_menu_toolbar": "^3.0@RC",
        "drupal/admin_views": "^1.7",
        "drupal/backup_migrate": "^3.9",
        "drupal/blockify": "^1.3",
        "drupal/boxes": "^1.2",
        "drupal/captcha": "^1.7",
        "drupal/coder": "^8.3",
        "drupal/composer_autoloader": "^1.0",
        "drupal/context": "^3.10",
        "drupal/context_ui": "^3.10",
        "drupal/ctools": "^1.19",
        "drupal/date": "^2.11",
        "drupal/date_api": "^2.11",
        "drupal/date_popup": "^2.11",
        "drupal/date_views": "^2.11",
        "drupal/devel": "^1.7",
        "drupal/diff": "^3.4",
        "drupal/drupal": "^7.62",
        "drupal/entity": "^1.9",
        "drupal/features": "^2.13",
        "drupal/feeds": "^2.0@beta",
        "drupal/feeds_tamper": "^1.2",
        "drupal/feeds_tamper_ui": "^1.2",
        "drupal/feeds_ui": "^2.0@beta",
        "drupal/field_group": "^1.6",
        "drupal/fieldable_panels_panes": "^1.13",
        "drupal/flag": "^3.9",
        "drupal/flag_abuse": "^2.0",
        "drupal/geocoder": "^1.7",
        "drupal/geofield": "^2.4",
        "drupal/geophp": "^1.8",
        "drupal/job_scheduler": "^2.0",
        "drupal/jquery_update": "^2.7",
        "drupal/languagefield": "^1.7",
        "drupal/leaflet": "^1.4",
        "drupal/leaflet_markercluster": "^1.4",
        "drupal/libraries": "^2.5",
        "drupal/mailsystem": "^2.35",
        "drupal/mandrill": "^2.4",
        "drupal/menu_block": "^2.8",
        "drupal/module_filter": "^2.2",
        "drupal/node_vertical_tab_elements": "^3.0",
        "drupal/page_manager": "^1.19",
        "drupal/panels": "^3.10",
        "drupal/prepopulate": "^2.1",
        "drupal/r4032login": "^1.8",
        "drupal/recaptcha": "^2.3",
        "drupal/rubik": "^4.4",
        "drupal/rules": "^2.12",
        "drupal/simplified_menu_admin": "^1.0",
        "drupal/strongarm": "^2.0",
        "drupal/tao": "^3.1",
        "drupal/term_merge": "^1.4",
        "drupal/token": "^1.8",
        "drupal/token_filter": "^1.1",
        "drupal/views": "^3.24",
        "drupal/views_bulk_operations": "^3.6",
        "drupal/views_content": "^1.19",
        "drupal/views_timelinejs": "^3.1",
        "drupal/views_ui": "^3.24",
        "drupal/webform": "^4.24",
        "drupal/zen": "5.6",
        "drush/drush": "^8.0",
        "symfony/filesystem": "~2.7 || ^3",
        "webflo/drupal-finder": "^1.2.1"
    },
    "conflict": {
        "drupal/core": "8.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-create-project-cmd": [
            "DrupalProject\\composer\\ScriptHandler::removeInternalFiles"
        ],
        "setup": [
            "composer install",
            "cp ./config/settings.php ./web/sites/default/settings.php"
        ],
        "drupal:install": [
            "./vendor/bin/drush -y si minimal"
        ],
        "drupal:update": [
            "./vendor/bin/drush -y updb"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/": ["type:drupal-core"],
            "web/profiles/{$name}/": ["type:drupal-profile"],
            "web/sites/all/drush/{$name}/": ["type:drupal-drush"],
            "web/sites/all/libraries/{$name}/": ["type:drupal-library"],
            "web/sites/all/modules/contrib/{$name}/": ["type:drupal-module"],
            "web/sites/all/themes/contrib/{$name}/": ["type:drupal-theme"]
        },
        "patches-install-hooks": true,
        "patches": {
            "cweagans/composer-patches": {
                "Call the preserve paths hooks" : "https://github.com/jcnventura/composer-patches/compare/1.x...jcnventura:fix-preserve-paths.diff"
            }
        },
        "preserve-paths": [
            "web/sites"
        ]
    }
}
